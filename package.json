{
  "name": "retext-dom",
  "version": "0.3.1",
  "description": "Create a (living) DOM tree from a TextOM tree",
  "license": "MIT",
  "keywords": [
    "dom",
    "retext"
  ],
  "dependencies": {
    "retext-visit": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/retext-dom.git"
  },
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "devDependencies": {
    "browserify": "^8.0.0",
    "eslint": "^0.12.0",
    "istanbul": "^0.3.0",
    "jscs": "^1.0.0",
    "jsdom": "^3.0.0",
    "matcha": "^0.6.0",
    "mocha": "^2.0.0",
    "retext": "^0.5.0",
    "retext-content": "^0.2.5",
    "retext-inspect": "^0.4.0"
  },
  "scripts": {
    "test-api": "_mocha --check-leaks test/index.js",
    "test-coveralls": "istanbul cover _mocha --report lcovonly -- --check-leaks test/index.js",
    "test-coverage": "istanbul cover _mocha -- --check-leaks test/index.js",
    "test-travis": "npm run test-coveralls",
    "test": "npm run test-api",
    "lint-api": "eslint index.js",
    "lint-benchmark": "eslint --global bench,before,suite,set benchmark.js",
    "lint-test": "eslint --env mocha test/index.js",
    "lint-style": "jscs --reporter inline index.js benchmark.js test/index.js",
    "lint": "npm run lint-api && npm run lint-benchmark && npm run lint-test && npm run lint-style",
    "make": "npm run lint && npm run test-coverage",
    "benchmark": "matcha benchmark.js",
    "build-browser": "browserify test/index.js -o test/browser.js",
    "build": "npm run build-browser"
  }
}
